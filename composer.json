{
    "name": "pbd-kn/contao-inputvar-bundle",
    "type": "contao-bundle",
    "description": "get/set Input Variables ",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        },
        {
            "name": "pbd-kn",
            "email": "pb-contao@gmx.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "pb-contao@gmx.de",
        "issues": "https://github.com/pbd-kn/contao-inputvar-bundle/issues",
        "source": "https://github.com/pbd-kn/contao-inputvar-bundle"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "contao/core-bundle": "^4.13 || ^5.0",
        "symfony/config": "^4.4 || ^5.1 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.1 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.1 || ^6.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^4.2",
        "phpunit/phpunit": "^8.4 || ^9",
        "symfony/phpunit-bridge": "^4.4 || ^5.1 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "PBDKN\\ContaoInputVarBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PBDKN\\ContaoInputVarBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "PBDKN\\ContaoInputVarBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ tests/ --fix --ansi"
        ]
    }
}
